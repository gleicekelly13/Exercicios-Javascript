function testeFun(param1, param2) {  // param1 e param2 s찾o par창metros
    console.log(param1 + param2);
}
testeFun("Gleice ", "Kelly"); //As strings "Gleice" e "Kelly" s찾o argumentos

//--------------------------------------------------------------------------//

Function somar(n1, n2) {
    let resultado = n1 + n2;
    console.log('Resultado: ' + resultado);
}
somar(10, 15); // 10 e 15 s찾o argumentos
